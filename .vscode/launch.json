	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug executable 'fuzz_target_1'",
			"cargo": {
				"args": ["build", "--bin=fuzz_target_1", "--package=weaveconfig-fuzz"],
				"filter": {
					"name": "fuzz_target_1",
					"kind": "bin"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in executable 'fuzz_target_1'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--bin=fuzz_target_1",
					"--package=weaveconfig-fuzz"
				],
				"filter": {
					"name": "fuzz_target_1",
					"kind": "bin"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in library 'weaveconfig'",
			"cargo": {
				"args": ["test", "--no-run", "--lib", "--package=weaveconfig"],
				"filter": {
					"name": "weaveconfig",
					"kind": "lib"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug executable 'cli'",
			"cargo": {
				"args": ["build", "--bin=cli", "--package=weaveconfig"],
				"filter": {
					"name": "cli",
					"kind": "bin"
				}
			},
			"args": ["./example"],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in executable 'cli'",
			"cargo": {
				"args": ["test", "--no-run", "--bin=cli", "--package=weaveconfig"],
				"filter": {
					"name": "cli",
					"kind": "bin"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		}
	]
}
